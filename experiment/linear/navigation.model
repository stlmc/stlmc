	(and (x <= 50) (y >= 50)) =>
	  (and (dx'= false) (dy' = false) (x' = x) (y' = y)); 
        (and (x >= 50) (y >= 50)) =>
          (and (dx'= false) (dy' = true) (x' = x) (y' = y));
}
# zone 4
#   x: increase, y: increase
{
    mode:
        dx = true;
        dy = true;
    inv:
        x >= 50;
        y <= 50;
    flow:
        d/dt[x] = 1;
        d/dt[y] = 4;
