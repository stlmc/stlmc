  jump:
        sqrt(x * x + y * y) >= 100 ==>
        @1 (and (x' = x) (vx' = vx) (ax' = ax) (tau' = tau) (y' = y) (vy' = vy) (ay' = ay));
        sqrt(x * x + y * y) <= 100 ==>
 @2 (and (x' = x) (vx' = vx) (ax' = ax) (tau' = tau) (y' = y) (vy' = vy) (ay' = ay)); 
        (and (tau >= 10.0) (tau <= 80.0)) ==>
 @3 (and (x' = x) (vx' = vx) (ax' = ax) (tau' = tau) (y' = y) (vy' = vy) (ay' = ay)); 
}
{mode 2;
  invt: (tau <= 80.0);
