[0, 150] x;
[0, 150] vx;
[0, 150] ax;
[0, 150] y;
[0, 150] vy;
[0, 150] ay;
[0, 100] tau;
[0, 1] time;
{mode 1;
  invt: (tau <= 80.0);
        (sqrt(x * x + y * y) >= 100);
  flow:
        d/dt[x] = vx;
        d/dt[vx] = ax;
        d/dt[ax] = 3 * 4.7351 * 4.7351 * x + 2 * 4.7351 * vy + 1.0 * -5.0 / 500.0;
        d/dt[y] = vy;
        d/dt[vy] = ay;
        d/dt[ay] = -2 * 4.7351 * vx + 1.0 * -5.0 / 500.0;
